{
    "name": "whispir-pairing-env",
    "version": "1.0.1",
    "description": "A starter package for interview testing",
    "repository": {
        "type": "git",
        "url": "https://www.whispir.com"
    },
    "author": "Whispir",
    "license": "UNLICENSE",
    "scripts": {
